{
  "redis_url": "redis://localhost:6379",
  "rate_limit": {
    "default_limit": 3,
    "time_window": 60,
    "excluded_roles": ["admin"],
    "key_config": {
      "from_headers": ["x-user-id", "x-client-id"],
      "from_session_variables": ["user.id"]
    },
    "unavailable_behavior": {
      "fallback_mode": "deny"
    }
  }
}
